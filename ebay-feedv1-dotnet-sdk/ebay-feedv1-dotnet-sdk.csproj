<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>eBay.sdk</RootNamespace>
    <PackageId>eBay.BuyFeedV1.Client</PackageId>
    <Version>0.0.3</Version>
    <PackageTags>eBay FeedV1API SDK</PackageTags>
    <Description>
      This SDK is intended to provide an easy way to filter for item in the latest feed file for a particular marketplace and category. It also provides a way to download the feed file.
    </Description>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="eBay.OAuth.Client" Version="2.0.3" />
    <PackageReference Include="YamlDotNet" Version="16.1.3" />
    <PackageReference Include="RestSharp" Version="112.1.0" />
    <PackageReference Include="log4net" Version="3.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />

  </ItemGroup>
</Project>
